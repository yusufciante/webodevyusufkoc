<h2>Yeni Branş  Ekle</h2>
@model DoktorBrans
<form method="post">
    <div class="border p-3 mt-5">
        <div class="row pb-3">
            <h2 class="text-primary"> Doktor Branşı Ekle</h2>
            </hr>
        </div>
        <div class="mb-3">
            <label asp-for="Ad" class="p-0"> </label>
            <input asp-for="Ad" class="form-control" />
            <span asp-validation-for="Ad" class="text-danger"></span>

        </div>

        <button type="submit" class="btn btn-lg btn-primary" style="width:250px">Doktor Branşı Ekle</button>

        <a asp-controller="DoktorBrans" asp-action="Index" class="btn btn-lg btn-primary style="width:250px">
            Doktor Branşlarına Geri Dön
        </a>


    </div>

</form>

